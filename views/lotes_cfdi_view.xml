<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="view_lotes_cfdi" model="ir.ui.view">

        <field name="name">Lotes Cfdi x View</field>
        <field name="model">lotes_cfdi</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="account_move_id" readonly="1"/>
                            <field name="lotes_id" readonly="1"/>

                        </group>
                        <group>

                        </group>
                    </group>
                </sheet>
            </form>

        </field>
    </record>

    <record id="lotes_cfdi_tree" model="ir.ui.view">

        <field name="name">Tree Lotes Cfdi View</field>
        <field name="model">lotes_cfdi</field>
        <field name="arch" type="xml">



                    <tree string="Lotes Cfdi View" multi_edit="1" limit="1000">
                        <field name="lotes_id" readonly="1"/>
                        <field name="abono" readonly="0" optional="1"/>
                        <field name="saldo_pendiente" readonly="1"/>
                        <field name="lotes_id_fecha" readonly="1"/>
                        <field name="lotes_id_partner_id" readonly="1"/>
                        <field name="lotes_id_importe"/>
                        <field name="account_move_id" readonly="1"/>
                        <field name="account_move_id_uuid" readonly="1"/>
                        <field name="account_move_id_partner_id" readonly="1"/>

                    </tree>



        </field>

    </record>

</odoo>