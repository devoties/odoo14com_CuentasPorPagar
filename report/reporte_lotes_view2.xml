<odoo>
    <template id="lotes_report_new">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <div class="page">
                    <div class="oe_structure"/>

                        <div style="text-align:center;"><h4 style="margin-bottom:16px;">Lotes View New</h4></div>


                    <div class="row">
                        <table class="table table_condensed">
                            <thead>
                                <tr>
                                    <th style="text-align:left;"><strong>Proveedor.: </strong></th>
                                    <th style="text-align:left;"><strong>Importe: </strong></th>
                                    <th style="text-align:left;"><strong>Conteo: </strong></th>

                                </tr>
                            </thead>
                            <tbody>
                                <tr t-foreach="vals" t-as="o">
                                    <td >
                                    <span t-esc="o['name']" />
                                    </td>
                                         <td >
                                    <span t-esc="o['imp']" />
                                    </td>
                                         <td >
                                    <span t-esc="o['conteo']" />
                                    </td>

                                </tr>

                            </tbody>
                        </table>
                    </div>
                </div>
            </t>
        </t>
    </template>

</odoo>
